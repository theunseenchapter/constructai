{
  "name": "tr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:turbo": "next dev --turbopack -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "dev:force": "powershell -Command \"$procs = netstat -ano | findstr ':3000' | ForEach-Object { ($_ -split '\\s+')[-1] } | Sort-Object -Unique; foreach ($id in $procs) { if ($id -and $id -ne '0') { Stop-Process -Id $id -Force -ErrorAction SilentlyContinue } }; next dev -p 3000\""
  },
